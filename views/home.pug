extends base.pug

block scripts
    scripts(src="../static/index.js")
block style
        style
            include ../static/style.css
            include ../static/style-responsive.css
block content
    section#introSections
        div.head  Welcome To AK STUDIO
        div.small A Place for Best Photos
    section#sponsersSections
        h2 Our Works
        div.card
            h3 #[a(href = '/wedding') Wedding Photoshoot]
            div.card-box
                div.card-img
                    img(src="../static/ak\ studio\ images/wedding/wedding2.jpeg",alt="Error")
            div.card-content
                p Have a look at Wedding photos
            //- img(src="../static/wedding.jpg", alt="../static/wedding2.jpg")-//


        div.card
            h3 #[a(href = '/Babyshoot') Baby Shoot]
            div.card-box
                div.card-img

                    img(src="../static/ak\ studio\ images/baby/baby1.jpg", alt="Error")
            div.card-content
                p Have a look at the cuteness of the baby.
            //- img(src="../static/wedding2.jpg", alt="")
        div.card
            h3 #[a(href = '/maternity-shot') Maternity Shoot]
            div.card-box
                div.card-img
                    img(src="../static/baby.jpg", alt="Error")
            div.card-content
